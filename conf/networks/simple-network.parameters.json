{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "metadata": {
    "comments": "Parameters for the network/simple-network template, custom but shared by all CaMeLz environments.",
    "author": "Michael Crawford <michael.crawford@neudesic.com>"
  },
  "parameters": {
    "companyName": {
      "value": "CaMeLz"
    },
    "environmentName": {
      "value": "Production"
    },
    "applicationName": {
      "value": "SFTP"
    },
    "componentName": {
      "value": "Network"
    },
    "vnetName": {
      "value": "SFTP-VNet"
    },
    "vnetAddressPrefix": {
      "value": "10.0.0.0/16"
    },
    "subnet1Prefix": {
      "value": "10.0.0.0/24"
    },
    "subnet1Name": {
      "value": "Subnet1"
    },
    "subnet2Prefix": {
      "value": "10.0.1.0/24"
    },
    "subnet2Name": {
      "value": "Subnet2"
  }
}
